[32m+[m[32m#include <iostream>[m
[32m+[m[32musing namespace std;[m
[32m+[m
[32m+[m[32mint noOfOneBits(int n) {[m
[32m+[m[32m    int cnt = 0;[m
[32m+[m[32m    for (int i = 0; i < 31; i++) {[m
[32m+[m[32m        if (n & (1 << i)) {[m
[32m+[m[32m            cnt++;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    return cnt;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mint main() {[m
