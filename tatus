[32m+[m[32m    cout << "Enter a number: ";[m
[32m+[m[32m    cin >> n;[m
[32m+[m[32m    cout << "Number of 1-bits: " << noOfOneBits(n) << endl;[m
[32m+[m[32m    return 0;[m
[32m+[m[32m}[m
